<!DOCTYPE html>
<html>
<head>
    <title>Productos</title>
</head>
<body>
    <h1>Bienvenido, {{user.email}}</h1>

    <h2>Productos:</h2>
    <ul>
        {{#each products}}
            <li>
                <h3>{{this.title}}</h3>
                <p>{{this.description}}</p>
                <p>Precio: {{this.price}}</p>
            </li>
        {{/each}}
    </ul>
</body>
</html>